{
  "name": "moxui",
  "private": true,
  "workspaces": [
    "packages/*",
    "play",
    "docs"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "pnpm run -C play dev",
    "docs:dev": "concurrently \"vitepress dev docs\" \"pnpm run -C docs-preview dev\"",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "bs": "esno ./scripts/tools/buildScss",
    "release": "esno ./scripts/tools/release",
    "initCmp": "esno ./scripts/tools/addNewComponent",
    "initPl": "esno ./scripts/tools/addNewPlugin",
    "rmCmp": "esno ./scripts/tools/removeComponent",
    "rmPl": "esno ./scripts/tools/removePlugin",
    "rmmo": "esno ./scripts/tools/clearNodeModules",
    "rmdt": "rm -rf ./dist"
  },
  "keywords": [],
  "author": "Liqun",
  "license": "MIT",
  "devDependencies": {
    "@algolia/client-search": "^4.14.2",
    "@types/lodash": "^4.14.185",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/eslint-config-prettier": "^7.0.0",
    "autoprefixer": "^10.4.8",
    "colors": "^1.4.0",
    "cpy": "^9.0.1",
    "eslint": "^8.23.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.4.0",
    "esno": "^0.16.3",
    "fast-glob": "^3.2.11",
    "postcss": "^8.4.16",
    "postcss-px-to-viewport-8-plugin": "^1.1.5",
    "prettier": "^2.7.1",
    "sass": "^1.54.8",
    "shelljs": "^0.8.5",
    "svga.lite": "^1.6.1",
    "typescript": "^4.8.2",
    "unplugin-vue-components": "^0.22.8",
    "vite": "^3.0.9",
    "vite-plugin-dts": "^1.4.1",
    "vitepress": "1.0.0-alpha.21",
    "vitepress-theme-demoblock": "^1.4.2",
    "vue": "^3.2.36",
    "vue-eslint-parser": "^9.0.3"
  },
  "dependencies": {
    "@moxui/components": "workspace:^1.0.0",
    "@moxui/constants": "workspace:^1.0.0",
    "@moxui/hooks": "workspace:^1.0.0",
    "@moxui/plugins": "workspace:^1.0.0",
    "@moxui/style": "workspace:^1.0.0",
    "@moxui/utils": "workspace:^1.0.0",
    "lodash": "^4.17.21",
    "svga.lite": "^1.6.1"
  },
  "peerDependencies": {
    "vue": "^3.2.36"
  }
}
